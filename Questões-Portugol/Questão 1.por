programa {
  funcao inicio() {
    cadeia nome
    inteiro idade
    real altura

    escreva("Digite o seu nome: ")
    leia(nome)

    escreva("Digite a sua idade: ")
    leia(idade)

    escreva("Digite a sua altura: ")
    leia(altura)

    escreva("\n", nome)
    escreva("\n", idade)
    escreva("\n", altura)
  }
}
